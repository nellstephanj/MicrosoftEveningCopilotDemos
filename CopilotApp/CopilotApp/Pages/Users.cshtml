@page
@model UsersModel
@{
    ViewData["Title"] = "All Users";
}

<h1>All Users</h1>
<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>User Name</th>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model.Users)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.UserName</td>
            <td>@user.Id</td>
        </tr>
    }
    </tbody>
</table>